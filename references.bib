journal = {NeuroImage},
keywords = {Diffusion weighted imaging,MC,Monte Carlo simulation,Myelination,White matter},
mendeley-tags = {MC},
pages = {212--220},
pmid = {23507381},
publisher = {Elsevier B.V.},
title = {{A computational model for diffusion weighted imaging of myelinated white matter}},
url = {http://dx.doi.org/10.1016/j.neuroimage.2013.02.076},
volume = {75},
year = {2013}
}
@article{Szafer1995,
abstract = {Water diffusion in a tissue model is studied both analytically and numerically. Tissue is regarded as a periodic array of boxes surrounded by partially permeable membranes (cells), embedded in an extracellular medium. intracellular and extracellular diffusion coefficients may differ. Expressions for the apparent diffusion coefficients (ADC) in isotropic and nonisotropic tissues are derived and compared with Monte Carlo simulations. Calculated ADCs disagree with values obtained from the widely used "fast exchange" formula. Effects of differences between intracellular and extracellular T2 relaxation times on measured values of ADC and T2 are discussed. The general analysis is specifically applied to the changes occurring in ADC following ischemic insults to brain tissue. It is found that although membranes affect ADC significantly, the observed changes in diffusion cannot be due to reduced membrane permeabilities. They may result from the combined effect of changes in cellular volume fraction, extracellular and intracellular diffusion.},
annote = {First (that I know) paper simulating diffusion MRI using Monte Carlo methods. 

Treat tissue as a periodic array of boxes with partially permeable membranes embeded in extracellular medium. 
Intra and extracellular components can have different diffusion coefficient and T2. 

Each spin carries a transverse magnetisation and T2 directly simulated at each step. 

Spins colliding with wall either reflected or transmitted with probability determined by the permeability. The probability is determined from the permeability according to [3].

Gradients are treated as instantaneous. The first one being applied immediately and the second being applied after Delta. There are examples with finite gradients however. 

Signal is measured as sum of transverse magnetisation immediately after second gradient.

Second Monte Carlo study I found. Here they simulate spinal cord white matter as permeable cylinders.

The idea is that they can measure the ADC parallel and perpendicular to fibres to see whether there's damange to the fibres. 

Cylinders are simulated on square and hexagonal lattices. 

Simulations are very similar to Szafer et. al 1995 but has two differences - T2 is ignored and rather than reflecting a particle, it is just left where it is. This is to prevent the need to track multiple reflections. 

Use a finite length gradient pulse. 

Compared results with analytical solutions for simple environments and also with Szafer et al.},
author = {Szafer, a and Zhong, J and Gore, J C},
doi = {10.1002/mrm.1910330516},
file = {:Users/rosscallaghan/Library/Application Support/Mendeley Desktop/Downloaded/Szafer, Zhong, Gore - 1995 - Theoretical model for water diffusion in tissues.pdf:pdf},
isbn = {0740-3194 (Print)},
issn = {0740-3194},
journal = {Magnetic resonance in medicine : official journal of the Society of Magnetic Resonance in Medicine / Society of Magnetic Resonance in Medicine},
keywords = {MC,brain,diffusion,nmr,stroke},
mendeley-tags = {MC},
number = {5},
pages = {697--712},
pmid = {7596275},
title = {{Theoretical model for water diffusion in tissues.}},
volume = {33},
year = {1995}
}
@article{Ford1997,
abstract = {We have implemented a numerical method for calculation of the apparent diffusion coefficient (ADC) in spinal cord injury, which takes into account the distribution of axon diameters and permeability found in spinal cord white matter, as well as relative axonal volume. We propose a procedure for determining the status of axonal integrity from measured ADC values. These methods have been applied to a well characterized rat spinal cord injury model, affording a prediction of the increase in axonal permeability which is presumed to be closely related to functional deficit. ADC values are compared to those calculated from analytical formulas in the literature, and possible factors underlying the ADC behavior are explored. Calculated results indicate both axonal swelling and cell membrane permeability to be important factors contributing to ADC in traumatic spinal cord injury.},
