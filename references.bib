@article{Grella2003,
abstract = {We present a three-dimensional simulation of scanning electron microscope$\backslash$n(SEM) images and surface charging. First, the field above the sample$\backslash$nis calculated using Laplace's equation with the proper boundary conditions;$\backslash$nthen, the simulation algorithm starts following the electron trajectory$\backslash$noutside the sample by using electron ray tracing. When the electron$\backslash$ncollides with the specimen, the algorithm keeps track of the electron$\backslash$ninside the sample by simulating the electron scattering history with$\backslash$na Monte Carlo code. During this phase, secondary and backscattered$\backslash$nelectrons are emitted to form an image and primary electrons are$\backslash$nabsorbed; therefore, a charge density is formed in the material.$\backslash$nThis charge density is used to recalculate the field above and inside$\backslash$nthe sample by solving the Poisson equation with the proper boundary$\backslash$nconditions. Field equation, Monte Carlo scattering simulation, and$\backslash$nelectron ray tracing are therefore integrated in a self-consistent$\backslash$nfashion to form an algorithm capable of simulating charging and imaging$\backslash$nof insulating structures. To maintain generality, this algorithm$\backslash$nhas been implemented in three dimensions. We shall apply the so-defined$\backslash$nsimulation to calculate both the global surface voltage and local$\backslash$nmicrofields induced by the scanning beam. Furthermore, we shall show$\backslash$nhow charging affects resolution and image formation in general and$\backslash$nhow its characteristics change when imaging parameters are changed.$\backslash$nWe shall address magnification, scanning strategy, and applied field.$\backslash$nThe results, compared with experiments, clearly indicate that charging$\backslash$nand the proper boundary conditions must be included in order to simulate$\backslash$nimages of insulating features. Furthermore, we shall show that a$\backslash$nthree-dimensional implementation is mandatory for understanding local$\backslash$nfield formation.},
author = {Grella, Luca and Lorusso, Gian and Adler, David L.},
file = {:Users/rosscallaghan/Documents/UCL/Papers{\_}General/Grella{\_}et{\_}al{\_}2003{\_}em{\_}sim.pdf:pdf},
issn = {01610457},
journal = {Scanning},
keywords = {Electron beam induced conductivity,Image simulation,Local fields,Monte Carlo simulation,Resist metrology,Secondary electron emission,Surface charging},
number = {6},
pages = {300--308},
title = {{Simulations of Scanning Electron Microscopy Imaging and Charging of Insulating Structures}},
volume = {25},
year = {2003}
}
@article{Ophus2017,
abstract = {Image simulation for scanning transmission electron microscopy at atomic resolution for samples with realistic dimensions can require very large computation times using existing simulation algorithms. We present a new algorithm named PRISM that combines features of the two most commonly used algorithms, the Bloch wave and multislice methods. PRISM uses a Fourier interpolation factor {\$}f{\$} that has typical values of 4-20 for atomic resolution simulations. We show that in many cases PRISM can provide a speedup that scales with {\$}f{\^{}}4{\$} compared to multislice simulations, with a negligible loss of accuracy. We demonstrate the usefulness of this method with large-scale scanning transmission electron microscopy image simulations of a crystalline nanoparticle on an amorphous carbon substrate.},
author = {Ophus, Colin},
doi = {10.1186/s40679-017-0046-1},
file = {:Users/rosscallaghan/Documents/UCL/Papers{\_}General/Ophus2017{\_}Article{\_}AFastImageSimulationAlgorithmF.pdf:pdf},
issn = {21980926},
journal = {Advanced Structural and Chemical Imaging},
keywords = {Electron scattering,Image simulation,Scanning transmission electron microscopy},
number = {1},
pages = {1--11},
publisher = {Springer International Publishing},
title = {{A fast image simulation algorithm for scanning transmission electron microscopy}},
volume = {3},
year = {2017}
}
@article{Kerr2008a,
abstract = {Many important physiological processes operate at time and space scales$\backslash$nfar beyond those accessible to atom-realistic simulations, and yet$\backslash$ndiscrete stochastic rather than continuum methods may best represent$\backslash$nfinite numbers of molecules interacting in complex cellular spaces.$\backslash$nWe describe and validate new tools and algorithms developed for a$\backslash$nnew version of the MCell simulation program (MCell3), which supports$\backslash$ngeneralized Monte Carlo modeling of diffusion and chemical reaction$\backslash$nin solution, on surfaces representing membranes, and combinations$\backslash$nthereof. A new syntax for describing the spatial directionality of$\backslash$nsurface reactions is introduced, along with optimizations and algorithms$\backslash$nthat can substantially reduce computational costs (e.g., event scheduling,$\backslash$nvariable time and space steps). Examples for simple reactions in$\backslash$nsimple spaces are validated by comparison to analytic solutions.$\backslash$nThus we show how spatially realistic Monte Carlo simulations of biological$\backslash$nsystems can be far more cost-effective than often is assumed, and$\backslash$nprovide a level of accuracy and insight beyond that of continuum$\backslash$nmethods.},
author = {Kerr, Rex A. and Bartol, Thomas M. and Kaminsky, Boris and Dittrich, Markus and Chang, Jen Chien Jack and Baden, Scott B. and Sejnowski, Terrence J. and Stiles, Joel R.},
doi = {10.1137/070692017},
file = {:Users/rosscallaghan/Documents/UCL/Papers{\_}General/Kerr{\_}et{\_}al{\_}2008{\_}MCell.pdf:pdf},
issn = {10648275},
journal = {SIAM Journal on Scientific Computing},
keywords = {Cell simulation,Concentration clamp,DReAMM,MCell,Microscopic reversibility,Monte carlo,Reaction-diffusion},
number = {6},
pages = {3126--3149},
title = {{Fast Monte Carlo simulation methods for biological reaction-diffusion systems in solution and on surfaces}},
volume = {30},
year = {2008}
}
@article{Menzel2015,
abstract = {The neuroimaging technique three-dimensional polarized light imaging (3D-PLI) provides a high-resolution reconstruction of nerve fibres in human post-mortem brains. The orientations of the fibres are derived from birefringence measurements of histological brain sections assuming that the nerve fibres - consisting of an axon and a surrounding myelin sheath - are uniaxial birefringent and that the measured optic axis is oriented in direction of the nerve fibres (macroscopic model). Although experimental studies support this assumption, the molecular structure of the myelin sheath suggests that the birefringence of a nerve fibre can be described more precisely by multiple optic axes oriented radially around the fibre axis (microscopic model). In this paper, we compare the use of the macroscopic and the microscopic model for simulating 3D-PLI by means of the Jones matrix formalism. The simulations show that the macroscopic model ensures a reliable estimation of the fibre orientations as long as the polarimeter does not resolve structures smaller than the diameter of single fibres. In the case of fibre bundles, polarimeters with even higher resolutions can be used without losing reliability. When taking the myelin density into account, the derived fibre orientations are considerably improved.},
author = {Menzel, M. and Michielsen, K. and {De Raedt}, H. and Reckfort, J. and Amunts, K. and Axer, M.},
doi = {10.1098/rsif.2015.0734},
file = {:Users/rosscallaghan/Documents/UCL/Papers{\_}General/Menzel{\_}et{\_}al{\_}2015{\_}3DPLI{\_}sim.pdf:pdf},
issn = {17425662},
journal = {Journal of the Royal Society Interface},
keywords = {Birefringence,Computer simulation,Jones matrix calculus,Nerve fibre architecture,Optics,Polarized light imaging},
number = {111},
title = {{A Jones matrix formalism for simulating three-dimensional polarized light imaging of brain tissue}},
volume = {12},
year = {2015}
}
@article{Plimpton1997,
abstract = {Three parallel algorithms for classical molecular dynamics are presented. The first assigns each processor a fixed subset of atoms; the second assigns each a fixed subset of inter-atomic forces to compute; the third assigns each a fixed spatial region. The algorithms are suitable for molecular dynamics models which can be difficult to parallelize efficiently-those with short-range forces where the neighbors of each atom change rapidly. They can be implemented on any distributed-memory parallel machine which allows for message-passing of data between independently executing processors. The algorithms are tested on a standard Lennard-Jones benchmark problem for system sizes ranging from 500 to 100,000,000 atoms on several parallel supercomputers-the nCUBE 2, Intel iPSC/860 and Paragon, and Cray T3D. Comparing the results to the fastest reported vectorized Cray Y-MP and C90 algorithm shows that the current generation of parallel machines is competitive with conventional vector supercomputers even for small problems. For large problems, the spatial algorithm achieves parallel efficiencies of 90{\%} and a 1840-node Intel Paragon performs up to 165 faster than a single Cray C90 processor. Trade-offs between the three algorithms and guidelines for adapting them to more complex molecular dynamics simulations are also discussed.},
archivePrefix = {arXiv},
arxivId = {10.1002/nag.2347},
author = {Plimpton, Steve},
doi = {10.1006/jcph.1995.1039},
eprint = {nag.2347},
