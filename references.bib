@article{Hagslatt2003,
abstract = {Pulsed field gradient NMR diffusometry is a promising tool for investigating structures of porous material through determinations of dynamic displacements of molecules in porous systems. A problem with this approach is the lack of closed analytical expressions for echo-decays in anything but idealized pore geometries. We present here an approach based on calculating the appropriate diffusion propagator by means of finite element calculations. The suggested method is quite general, and can be applied to arbitrary porous systems. The protocol for the calculations is outlined and we show results from some different cases: diffusion in confined geometries and in systems that are spatially inhomogeneous with respect to concentration. {\textcopyright} 2003 Elsevier Science (USA). All rights reserved.},
